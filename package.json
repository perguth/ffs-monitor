{
  "name": "ffs-monitor",
  "version": "1.1.0",
  "description": "Monitor your freifunk devices.",
  "author": "Per Guth <mail@perguth.de>",
  "license": "MIT",
  "keywords": [
    "freifunk",
    "stuttgart",
    "monitoring"
  ],
  "main": "main.js",
  "browser": "browser.js",
  "scripts": {
    "test": "standard",
    "start": "REST_URL=http://localhost:9000 WS_URL=$REST_URL npm run browser & npm run server",
    "browser": "budo --live --host localhost browser.js:bundle.js -- -t envify",
    "build": "mkdir -p build && browserify browser.js -o build/bundle.js -t envify -t babelify --presets [es2015] && cp -r index.html assets build/ && touch build/.nojekyll",
    "server": "nodemon server.js"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "budo": "^10.0.4",
    "nodemon": "^1.12.1",
    "standard": "^10.0.3"
  },
  "dependencies": {
    "backoff": "^2.5.0",
    "bootstrap": "^3.3.7",
    "choo": "^6.4.1",
    "choo-log": "^7.2.1",
    "choo-persist": "^3.0.2",
    "cors": "^2.8.4",
    "debug": "^3.1.0",
    "envify": "^4.1.0",
    "express": "^4.16.2",
    "fast-memoize": "^2.2.8",
    "moment": "^2.19.1",
    "nanocomponent": "^6.4.2",
    "node-fetch": "^1.7.3",
    "popper.js": "^1.12.5",
    "socket.io": "^2.0.3",
    "utf-8-validate": "^3.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/perguth/ffs-monitor.git"
  },
  "bugs": {
    "url": "https://github.com/perguth/ffs-monitor/issues"
  },
  "homepage": "https://github.com/perguth/ffs-monitor#readme"
}
